<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Item.
    -->
    <changeSet id="20191016005803-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="produto_id"
                                 baseTableName="item"
                                 constraintName="fk_item_produto_id"
                                 referencedColumnNames="id"
                                 referencedTableName="produto"/>

        <addForeignKeyConstraint baseColumnNames="venda_id"
                                 baseTableName="item"
                                 constraintName="fk_item_venda_id"
                                 referencedColumnNames="id"
                                 referencedTableName="venda"/>

    </changeSet>
</databaseChangeLog>
